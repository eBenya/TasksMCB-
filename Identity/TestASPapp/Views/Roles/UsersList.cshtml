@model IEnumerable<TestASP.Models.HelpModel.User>

<div class="container">
    <h2>Users list</h2>
    <table class="table" border="1">
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>
                    <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-userid="@user.Id">Access Permitions</a>
                </td>
            </tr>
        }
    </table>
</div>